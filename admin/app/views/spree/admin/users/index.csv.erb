<%- headers = [
  'First Name',
  'Last Name',
  'Email',
  'Accepts Email Marketing',
  'Company',
  'Address 1',
  'Address 2',
  'City',
  'Province',
  'Province Code',
  'Country',
  'Country Code',
  'Zip',
  'Phone',
  'Accepts SMS Marketing',
  'Total Spent',
  'Total Orders',
  'Tags',
  'Tax Exempt'
  ]
-%>
<%= CSV.generate_line(headers).strip %>
<%- @users.includes( bill_address: [:country], ship_address: [:country], addresses: [:country]).each do |user| -%>
<%= CSV.generate_line([
  user.first_name,
  user.last_name,
  user.email,
  user.accepts_email_marketing ? 'Yes' : 'No',
  user.address&.company,
  user.address&.address1,
  user.address&.address2,
  user.address&.city,
  user.address&.state_text,
  user.address&.state_abbr,
  user.address&.country&.name,
  user.address&.country&.iso,
  user.address&.zipcode,
  user.phone,
  user.accepts_sms_marketing ? 'Yes' : 'No',
  user.amount_spent_in(current_currency),
  user.completed_orders_count,
  user.tag_list,
  user.tax_exempt ? 'Yes' : 'No'
  ]).strip.html_safe %>
<%- end -%>

